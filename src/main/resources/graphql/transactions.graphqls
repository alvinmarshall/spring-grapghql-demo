type Transactions{
    id: ID,
    bonusAmount: BigDecimal,
    exchangeRate: BigDecimal,
    feeAmount: BigDecimal,
    fromAmount: BigDecimal,
    fromCurrency: String,
    toAmount: BigDecimal,
    toCurrency: String,
    reason: String,
    couponCode: String,
    status: String,
    senderId:ID,
    userId:ID,
}

enum TransactionStatus{
    DELIVERED,
    CANCELLED,
    FAILED,
    INITIATED
}

enum TransactionType{
    ACH,
    INSTANT
}
