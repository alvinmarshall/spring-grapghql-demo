type Customer{
    id: ID,
    firstName: String
    lastName: String
    email: String,
    recipients: [Recipient],
    totalTransactions: Int,
    totalTransactionsDelivered: Int,
    totalTransactionsCancelled: Int,
    totalTransactionsFailed: Int,
    numberOfRecipientsOfEachCountry: [RecipientGroupCountry]
}

type Recipient{
    id: ID,
    firstName: String,
    lastName: String
}

type RecipientGroupCountry{
    count: Int,
    country: String
}
